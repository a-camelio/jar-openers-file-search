<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefHeight="388.0" prefWidth="636.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jaropeners.SecondaryController">
    <children>
        <Label prefHeight="45.0" prefWidth="269.0" text="Index Maintenance">
         <font>
            <Font size="30.0" />
         </font></Label>
      <TableView prefHeight="400.0" prefWidth="596.0">
        <columns>
          <TableColumn prefWidth="299.0" text="File Name" />
          <TableColumn prefWidth="296.0" text="Status" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" spacing="100.0">
         <children>
            <Button mnemonicParsing="false" text="Add File" />
            <Button mnemonicParsing="false" text="Remove Selected Files" />
            <Button mnemonicParsing="false" text="Remove Out of date" />
              <Button fx:id="secondaryButton" onAction="#switchToPrimary" prefHeight="25.0" prefWidth="123.0" text="Back to File Search" />
         </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
